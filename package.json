    "formik": "^2.4.3",
    "i18next": "^23.4.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "metismenujs": "^1.4.0",
    "moment": "^2.29.4",
    "node-sass": "^9.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.3",
    "react-redux": "^8.1.2",
